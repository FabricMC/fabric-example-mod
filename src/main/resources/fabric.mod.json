{
	"schemaVersion": 1,
	"id": "fishingbell",
	"version": "${version}",
	"name": "Fishing Bell",
	"description": "Hear a bell ring when your fishing rod catches something and you have a bell in your offhand!",
	"authors": [
		"LilTOJustice"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/fishingbell/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.fishingbell.FishingBell"
		]
	},
	"mixins": [
		"fishingbell.mixins.json",
		{
			"config": "modid.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	},
	"accesswidener": {
		"accesswidener" : "fishingbell.accesswidener"
	}
}